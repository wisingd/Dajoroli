package com.example.splitit;

import java.io.Serializable;


public class Contact implements Serializable{
	
	String name;
	int debt;
	
	public  Contact(String name){
		
		name = this.name;
		debt = 0;
		
	}
	
	public void accumulateDebt(int debt){
		
		this.debt += debt;
		
	}
	public String getName(){
		return name;
	}
	public int getDebt(){
		return debt;
	}
	
	
}
